        http://mirror.rackspace.com/epel/6/$basearch/debug
        http://download.fedoraproject.org/pub/epel/6/$basearch/debug
failovermethod=priority
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-6
